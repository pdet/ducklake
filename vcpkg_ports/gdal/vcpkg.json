{
  "name": "gdal",
  "version-semver": "3.8.5",
  "description": "The Geographic Data Abstraction Library for reading and writing geospatial raster and vector data",
  "homepage": "https://gdal.org",
  "license": null,
  "supports": "!uwp",
  "dependencies": [
    "json-c",
    "libgeotiff",
    {
      "name": "pkgconf",
      "host": true
    },
    {
      "name": "proj",
      "default-features": false,
      "features": [
        "tiff"
      ]
    },
    {
      "name": "tiff",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-pkgconfig-get-modules",
      "host": true
    },
    "zlib"
  ],
  "features": {
    "geos": {
      "description": "Enable GEOS support",
      "dependencies": [
        {
          "name": "geos",
          "default-features": false
        }
      ]
    },
    "network": {
      "description": "Enable network support",
      "dependencies": [
        {
          "name": "curl",
          "platform": "!wasm32 & !ios & !android",
          "features": [
            "openssl"
          ]
        }
      ]
    }
  }
}
